RewriteEngine On
RewriteBase /

RewriteCond %{REQUEST_FILENAME} -d [OR]
RewriteCond %{REQUEST_FILENAME} -f
RewriteRule ^ - [L]

RewriteRule ^([^/]+)/([^/]+)/?$ index.php?redirect=$1&brand=$2 [L,QSA]
RewriteRule ^([^/]+)/?$ index.php?redirect=$1 [L,QSA]
